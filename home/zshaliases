# Filesystem
alias ..="cd .."
alias ...="cd ../.."
alias -- -="cd -"
alias c="clear"
alias l="ls -lah"
alias ll="l"
alias md="mkdir -p"

# Misc
alias be="bundle exec"
alias mysql="mysql --i-am-a-dummy"

# Git
alias gs="git status"
alias ga="git add"
alias gb="git branch"
alias gc="git commit"
alias gd="git diff"
